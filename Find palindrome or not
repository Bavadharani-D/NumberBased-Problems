import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc =new Scanner(System.in); 
      int n = sc.nextInt();
      int ans =  n;
      int rev=0;
      while(n>0){
        int f= n%10;
        rev = rev*10 +f;
        n=n/10;
}
      System.out.print((ans==rev) ? "Palindrome" : "Not a Palindrome");
    }}
