import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc =new Scanner(System.in); 
      int n = sc.nextInt();
      int f = n;
      int rev=0;
      while(n>0){
        f= n%10;
        rev = rev*10 +f;
        n=n/10;
}
      System.out.print(rev);
    }}
