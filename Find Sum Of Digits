import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc =new Scanner(System.in); 
      int n = sc.nextInt();
      int f = n;
      int sum=0;
      while(n>0){
        f=n%10;
        sum=sum+f;
        n=n/10;
      }
      System.out.println(sum);
  }
}
